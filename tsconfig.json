{
    "compilerOptions": {
        "target": "es5",
        "declaration": true,
        "module": "commonjs",
        "sourceMap": true,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": ["es2015", "dom"],
        "outDir": "dist",
        "noImplicitAny": false,
        "rootDir": "src"
    },
    "exclude": [
        "node_modules",
        "build",
        "dist"
    ]
}
